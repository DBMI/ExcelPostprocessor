<workbook>
    <name>test_data.xlsx</name>
    <sheet>
        <name>Labs</name>
        <source_column>
            <name>REPORT</name>
            <extract>
                <pattern>pH: ?(\d+\.?\d*)</pattern>
                <pattern>(\d+\.?\d*) ?pH</pattern>
                <new_column>pH</new_column>
            </extract>
            <extract>
                <pattern>TDS: ?(\d+\.?\d*)</pattern>
                <new_column>TDS</new_column>
            </extract>
        </source_column>
    </sheet>
</workbook>